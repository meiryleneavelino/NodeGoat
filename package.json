{
  "name": "owasp-nodejs-goat",
  "private": true,
  "version": "1.3.1",
  "description": "A tool to learn OWASP Top 10 for Node.js developers",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "cross-env PORT=5000 nodemon",
    "test:e2e": "cross-env NODE_ENV=test cypress open",
    "test:ci": "cross-env NODE_ENV=test cypress run",
    "test": "grunt test",
    "db:seed": "cross-env NODE_ENV=test grunt db-reset",
    "precommit": "grunt precommit",
    "docker-mongo": "docker run -p 27017:27017 --name mongo mongo:4.4",
    "start-infra": "docker-compose up",
    "stop-infra": "docker-compose down",
    "cy:verify": "cypress verify"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "consolidate": "^0.17.3",
    "csurf": "^1.11.0",
    "dont-sniff-mimetype": "^1.1.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "helmet": "^7.0.0",
    "marked": "^12.0.2",
    "mongodb": "^4.15.0",
    "needle": "^3.2.0",
    "serve-favicon": "^2.5.0",
    "underscore": "^1.13.6",
    "esapi-node": "^0.1.1"
  },
  "devDependencies": {
    "async": "^3.2.5",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.6",
    "grunt": "^1.6.1",
    "grunt-cli": "^1.4.3",
    "grunt-concurrent": "^2.3.1",
    "grunt-contrib-jshint": "^3.2.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-env": "^0.4.5",
    "grunt-if": "https://github.com/binarymist/grunt-if/tarball/master",
    "grunt-jsbeautifier": "^0.2.13",
    "grunt-mocha-test": "^0.13.3",
    "grunt-npm-install": "^0.3.0",
    "grunt-retire": "^0.3.14",
    "jshint": "^2.13.6",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.22",
    "selenium-webdriver": "^4.21.0",
    "should": "^13.2.3",
    "zaproxy": "^0.2.0"
  },
  "repository": "https://github.com/OWASP/NodejsGoat",
  "license": "Apache-2.0"
}
